\WRefstepcounter{subsubsubsection}%
\@@startsection{subsubsubsection}{4}{\z@@}{-3.25ex plus-1ex minus-.2ex}
{\Wuparindent}{\reset@@font \normalsize \bf}}%
			\global\let\Wunnamedsect\subsubsubsection
